 class callstaticmethod
 {
      static void method1()//stsic method 
      {
       
       System.out.println("This is static method 1");

      }
      
      static void method2()//stsic method  
      {
       
       System.out.println("This is static method 2");

      }
      
      static void method3()//stsic method  
      {
       
       System.out.println("This is static method 3");

      }
      
       void method4()// Non stsic method
      {
       
       System.out.println("This is non-static method 4");

      }
      
       void method5()// Non stsic method
      {
       
       System.out.println("This is non-static method 4");

      }
      
      void method6()// Non stsic method 
      {
       
       System.out.println("This is non-static method 4");

      }
      
      public static void main(String[] args) 
      
      {
        callstaticmethod Call_Non_Static = new callstaticmethod();
        System.out.println("This is main method");
        method1();
        method2();
        method3();
        Call_Non_Static.method4();
        Call_Non_Static.method5();
        Call_Non_Static.method6();
        
      }
 }
